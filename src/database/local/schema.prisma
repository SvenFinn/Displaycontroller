generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("SELF_DB_URL")
}

model Parameter {
    key       String   @id
    strValue  String?
    boolValue Boolean?
    numValue  Float?
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now()) @updatedAt
}
